<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.waicool20.kaga.views.tabs.sortie.SortieTabView">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <GridPane fx:id="content" GridPane.rowIndex="1">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="60.0" minHeight="60.0" prefHeight="60.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <HBox alignment="CENTER">
                            <children>
                                <Label fx:id="engineLabel" text="Engine:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1">
                     <children>
                        <ComboBox fx:id="engineComboBox" prefHeight="28.0" prefWidth="180.0" />
                     </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="1">
                            <children>
                                <Label fx:id="mapLabel" text="Map:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <children>
                                <ComboBox fx:id="mapComboBox" prefHeight="28.0" prefWidth="75.0" />
                                <Label fx:id="nodesLabel" text="Nodes:" />
                                <Spinner fx:id="nodesSpinner" editable="true" prefWidth="75.0" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="3">
                            <children>
                                <Label fx:id="nodeSelectsLabel" text="Node Selects:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="3">
                            <children>
                                <HBox alignment="CENTER_LEFT">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#onConfigureNodeSelectsButton" text="Configure" />
                                    </children>
                                </HBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="4">
                            <children>
                                <Label fx:id="formationsLabel" text="Formations:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="4">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onConfigureFormationsButton" text="Configure" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="5">
                            <children>
                                <Label fx:id="nightBattlesLabel" text="Night Battles:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="5">
                            <children>
                                <Button mnemonicParsing="false" onAction="#onConfigureNightBattlesButton" text="Configure" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="6">
                            <children>
                                <Label fx:id="retreatLimitLabel" text="Retreat Limit:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="6">
                            <children>
                                <ComboBox fx:id="retreatLimitComboBox" prefHeight="28.0" prefWidth="180.0" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="7">
                            <children>
                                <Label fx:id="repairLimitLabel" text="Repair Limit:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="7">
                            <children>
                                <ComboBox fx:id="repairLimitComboBox" prefHeight="28.0" prefWidth="180.0" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="8">
                            <children>
                                <Label fx:id="repairTimeLimitLabel" text="Repair &#10;Time Limit:" textAlignment="CENTER" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="8">
                            <children>
                                <HBox alignment="CENTER_LEFT">
                                    <children>
                                        <Spinner fx:id="repairTimeHourSpinner" editable="true" prefHeight="56.0" prefWidth="50.0" styleClass="split-arrows-vertical" />
                                        <Label text=":" />
                                        <Spinner fx:id="repairTimeMinSpinner" editable="true" prefHeight="56.0" prefWidth="50.0" styleClass="split-arrows-vertical" />
                                    </children>
                                </HBox>
                        <CheckBox fx:id="reserveDocksCheckBox" mnemonicParsing="false" text="Reserve Dock">
                           <HBox.margin>
                              <Insets left="5.0" />
                           </HBox.margin>
                        </CheckBox>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" GridPane.rowIndex="9">
                            <children>
                                <Label fx:id="checksLabel" text="Checks:" />
                            </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="9">
                            <children>
                                <CheckBox fx:id="checkFatigueCheckBox" mnemonicParsing="false" text="Fatigue" />
                                <CheckBox fx:id="checkPortCheckBox" mnemonicParsing="false" text="Port" />
                                <CheckBox fx:id="medalStopCheckBox" mnemonicParsing="false" text="Medal" />
                            </children>
                        </HBox>
                  <HBox alignment="CENTER" GridPane.rowIndex="2">
                     <children>
                        <Label fx:id="mapLabel1" text="Fleet Mode:" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER_LEFT" spacing="10.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <children>
                        <ComboBox fx:id="fleetModeComboBox" prefHeight="28.0" prefWidth="180.0" />
                     </children>
                  </HBox>
                    </children>
                </GridPane>
                <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="45.0" prefWidth="300.0" spacing="10.0">
                    <children>
                        <CheckBox fx:id="enableButton" mnemonicParsing="false" text="Enable" />
                    </children>
                    <opaqueInsets>
                        <Insets />
                    </opaqueInsets>
                    <padding>
                        <Insets left="20.0" />
                    </padding>
                </HBox>
            </children>
        </GridPane>
    </children>
</AnchorPane>
