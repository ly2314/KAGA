                  <Insets left="10.0" />
               </HBox.margin></Spinner>
            <Label text=":" />
            <Spinner fx:id="scriptStopTimeMinSpinner" editable="true" prefHeight="56.0" prefWidth="50.0" styleClass="split-arrows-vertical" />
         </children>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
      <Separator prefWidth="200.0">
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding>
      </Separator>
      <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0">
         <children>
            <CheckBox fx:id="enableExpStopCheckBox" mnemonicParsing="false" prefWidth="200.0" text="Enable Expedition Stop" />
            <ComboBox fx:id="expStopModeComboBox" prefWidth="125.0" />
