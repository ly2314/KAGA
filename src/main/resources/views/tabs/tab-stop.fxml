<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.waicool20.kaga.views.tabs.StopTabView">
   <children>
      <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="45.0">
         <children>
            <CheckBox fx:id="enableScriptStopCheckBox" mnemonicParsing="false" prefWidth="200.0" text="Enable Script Stop" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
      <HBox fx:id="scriptStopContent" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="75.0">
         <children>
            <Label text="Count:" />
            <Spinner fx:id="scriptStopCountSpinner" editable="true" prefWidth="100.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></Spinner>
            <Label text="Time:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></Label>
            <Spinner fx:id="scriptStopTimeHourSpinner" editable="true" prefHeight="56.0" prefWidth="50.0" styleClass="split-arrows-vertical">
