         <children>
            <CheckBox fx:id="enableSortieStopCheckBox" mnemonicParsing="false" prefWidth="200.0" text="Enable Sortie Stop" />
            <ComboBox fx:id="sortieStopModeComboBox" prefWidth="125.0" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets left="20.0" />
         </padding>
      </HBox>
      <HBox fx:id="sortieStopContent" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="75.0">
         <children>
            <Label text="Count:" />
